  "devDependencies": {
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.1",
    "faker": "^4.1.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2"
  },
  "dependencies": {
    "@pulumi/aws": "^0.18.26",
    "@pulumi/pulumi": "^0.17.28",
    "graphql": "^14.4.2",
    "merge-graphql-schemas": "^1.7.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/services/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/",
      "<rootDir>/build"
    ]
  }
