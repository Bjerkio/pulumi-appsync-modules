{
  "name": "pulumi-appsync-modules",
  "version": "2.0.3",
  "description": "AWS AppSync module for Pulumi /with Modules",
  "main": "build/index.js",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "test": "PULUMI_TEST_MODE=true PULUMI_NODEJS_STACK=test-ws PULUMI_NODEJS_PROJECT=dev jest --coverage src",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "generate:docs": "typedoc --mode file --readme none --plugin typedoc-plugin-markdown --hideSources --out docs src",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bjerkio/pulumi-appsync-modules.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bjerkio/pulumi-appsync-modules/issues"
  },
  "homepage": "https://github.com/Bjerkio/pulumi-appsync-modules#readme",
  "devDependencies": {
    "@types/faker": "^5.1.2",
    "@types/jest": "^26.0.3",
    "@types/node": "^12.7.1",
    "faker": "^5.1.0",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.1",
    "typedoc": "^0.20.1",
    "typedoc-plugin-markdown": "^3.0.8",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@graphql-toolkit/schema-merging": "^0.10.6",
    "@pulumi/aws": "^4.1.0",
    "@pulumi/pulumi": "^2.2.1",
    "graphql": "^15.0.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/",
      "<rootDir>/build"
    ]
  }
}
