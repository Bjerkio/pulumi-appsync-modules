{
  "name": "pulumi-appsync-modules",
  "version": "0.0.0-development",
  "description": "AWS AppSync module for Pulumi /with Modules",
  "main": "build/index.js",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "test": "PULUMI_TEST_MODE=true PULUMI_NODEJS_STACK=test-ws PULUMI_NODEJS_PROJECT=dev jest --coverage src",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bjerkio/pulumi-appsync-modules.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bjerkio/pulumi-appsync-modules/issues"
  },
  "homepage": "https://github.com/Bjerkio/pulumi-appsync-modules#readme",
  "devDependencies": {
    "@types/faker": "^4.1.5",
    "@types/graphql": "14.2.3",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.1",
    "faker": "^4.1.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "semantic-release": "^15.13.21"
  },
  "dependencies": {
    "graphql": "^14.4.2",
    "graphql-toolkit": "^0.7.5",
    "typescript": "^3.4.5",
    "@pulumi/aws": "^1.0.0",
    "@pulumi/pulumi": "^1.0.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/",
      "<rootDir>/build"
    ]
  }
}
